<?php
abstract class indicator
{
   /* Данный метод должен быть определён в дочернем классе */
    abstract public function weight();
    abstract public function height();
    abstract public function strength();

}




class warrior extends indicator
{
    public function weight() {
        
        echo "80". "\n";
    }

    public function height() {
        
        echo "190". "\n";
    }

    public function strength() {
        
        echo "80". "\n";
    }
}

class magick extends indicator
{
      public function weight() {
        echo "70" . "\n";
    }

    public function height() {
        echo "175" . "\n";
    }

    public function strength() {
        echo "40" . "\n";
    }
}

class bogatir extends indicator
{
      public function weight() {
        echo "100". "\n";
    }

    public function height() {
        echo "180". "\n";
    }

    public function strength() {
        echo "120". "\n";
    }
}


$class1 = new warrior;
 $class1->weight();	
 $class1->height();	
 $class1->strength();	
echo "</br>";


$class2 = new magick;
 $class2->weight();	
 $class2->height();	
 $class2->strength();	
echo "</br>";

$class3 = new bogatir;
 $class3->weight();	
 $class3->height();	
 $class3->strength();	
echo "</br>";


?>


